    <command:details>
      <command:name>Get-PSFunctionInfo</command:name>
      <command:verb>Get</command:verb>
      <command:noun>PSFunctionInfo</command:noun>
      <maml:description>
        <maml:para>Get metadata for stand-alone functions</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Display function metadata. The default behavior is to display loaded functions that don't belong to a module. Items like versioning belong to the module, not the individual function.</maml:para>
    </maml:description>
